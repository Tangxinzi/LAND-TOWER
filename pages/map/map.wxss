.contact {
  position: relative;
  z-index: 1;
}

.contact .content {
  font-size: 14px;
  padding: 1em;
  width: 100%;
  box-sizing: border-box;
  color: #FFF;
}

.contact .content .flex-between {
  margin-bottom: 5px;
}

map {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 70vh;
}